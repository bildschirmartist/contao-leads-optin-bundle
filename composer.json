{
  "name": "cboelter/contao-lead-optin",
  "description": "Opt-In functions for the contao-leads extension",
  "keywords": [
    "contao",
    "leads",
    "contao leads optin",
    "optin"
  ],
  "type": "contao-module",
  "homepage": "https://boelter.eu",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Christopher BÃ¶lter",
      "email": "christopher@boelter.eu",
      "homepage": "http://boelter.eu",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/cboelter/contao-leads-optin"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.9",
    "codefog/contao-haste": "^4.25",
    "terminal42/contao-leads": "~1.4",
    "terminal42/notification_center": "~1.4",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "Boelter\\LeadsOptin\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.4.x-dev"
    },
    "contao-manager-plugin": "Boelter\\LeadsOptin\\ContaoManager\\Plugin"
  }
}
